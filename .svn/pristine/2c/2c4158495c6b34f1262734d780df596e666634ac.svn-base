package co.com.grupoasd.nomina.modelo;

/**
 *
 * @author Grupo ASD
 */
public class TipoDocumento {
    private String codigo;
    private String descripcion;
    private String validoCargo;

    /**
     * @return the codigo
     */
    public String getCodigo() {
        return codigo;
    }

    /**
     * @param codigo the codigo to set
     */
    public void setCodigo(String codigo) {
        this.codigo = codigo;
    }

    /**
     * @return the descripcion
     */
    public String getDescripcion() {
        return descripcion;
    }

    /**
     * @param descripcion the descripcion to set
     */
    public void setDescripcion(String descripcion) {
        this.descripcion = descripcion;
    }

    /**
     * @return the validoCargo
     */
    public String getValidoCargo() {
        return validoCargo;
    }

    /**
     * @param validoCargo the validoCargo to set
     */
    public void setValidoCargo(String validoCargo) {
        this.validoCargo = validoCargo;
    }
    
    
}    
