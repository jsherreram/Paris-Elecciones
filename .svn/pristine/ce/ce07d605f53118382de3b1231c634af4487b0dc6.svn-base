<div class="content animate-panel">
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    <h3> Generar Carnet Individual</h3>
                </div>
                <div class="panel-body">

                    <form  ng-submit="controller.generarCarnets()" method="post" target="_blank">

                        <div class="form-group col-md-6">
                            <label style="color: #f22">*</label>
                            <label id="cargo" data-toggle="popover" data-content="El Cargo es requerido">Documento(s) separados por coma(,). Ejemplo:123896,456365</label>
                            <textarea type="text" ng-model="controller.nrodoc" class="form-control"></textarea>                                                   
                        </div>

                        <div class="form-group col-md-3">
                            <br> 
                            <input class="btn btn-primary" type="submit" value ="Generar">
                        </div>

                    </form>
                </div>
            </div>
        </div>
    </div>


    <div class='row'>
        <div class="col-md-12">
            <iframe class="embed-responsive-item"  width="100%" height="600"  frameborder="0" allowfullscreen ng-src="{{content}}"></iframe>
        </div>
    </div>    

</div>