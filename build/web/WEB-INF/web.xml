            <url-pattern>/styles/*</url-pattern>
            <url-pattern>/vendor/*</url-pattern>
            <url-pattern>/images/*</url-pattern>
            <url-pattern>/icons/*</url-pattern>
        </web-resource-collection>
    </security-constraint>
