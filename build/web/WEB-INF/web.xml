            <url-pattern>/panel/*</url-pattern>
        </web-resource-collection>
        <auth-constraint>
            <role-name>operador</role-name>
            <role-name>examinador</role-name>
        </auth-constraint>
    </security-constraint>
    <security-constraint>
        <web-resource-collection>
