<div class="content animate-panel">
    <div class="row">
        <div class="col-lg-12">
            <div class="hpanel">
                <div class="panel-body">                   
                    <h2 class="font-light m-b-xs">
                        Generar Carne
                    </h2>
                    
                </div>
            </div>
        </div>
    </div>
    
    <div class="stats-label text-color">
        <span class="font-extra-bold font-uppercase"></span>
    </div>

    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-primary">
              <div class="panel-heading">
              </div>
              <div class="panel-body">

                <form class="form-horizontal" action="${pageContext.request.contextPath}/Carnet" method="post" enctype="multipart/form-data">
                    
                    <div class="form-group">
                        <label class="control-label col-md-2" for="cargo">Cargo</label>
                        <div class="col-md-6">                            
                            <SELECT name="cargo" SIZE="1"> 

                               <OPTION VALUE="ARCHIVO">ARCHIVO</OPTION>
                               <OPTION VALUE="AUXILIAR DE PRENSA">AUXILIAR DE PRENSA</OPTION>
                               <OPTION VALUE="AUDITOR EXTERNO">AUDITOR EXTERNO</OPTION>
                               <OPTION VALUE="AUDITOR DE PARTIDOS">AUDITOR DE PARTIDOS</OPTION>
                               <OPTION VALUE="CENTRO DE COMPUTO">CENTRO DE COMPUTO</OPTION>
                               <OPTION VALUE="CLASIFICADOR">CLASIFICADOR</OPTION>
                               <OPTION VALUE="CONTRALOR">CONTRALOR</OPTION>
                               <OPTION VALUE="CONTROL DE ASISTENCIA">CONTROL DE ASISTENCIA</OPTION> 
                               <OPTION VALUE="COORDINADOR">COORDINADOR ESCRUTINIOS</OPTION> 
			       <OPTION VALUE="COORDINADOR PRECONTEO">COORDINADOR PRECONTEO</OPTION>
                               <OPTION VALUE="CORRECTOR">CORRECTOR</OPTION> 
                               <OPTION VALUE="DELEGADO DEPARTAMENTAL">DELEGADO DEPARTAMENTAL</OPTION>
                               <OPTION VALUE="DIGITALIZADOR">DIGITALIZADOR</OPTION>
                               <OPTION VALUE="DIRECTOR ADMINISTRATIVO">DIRECTOR ADMINISTRATIVO</OPTION>
                               <OPTION VALUE="DIRECTOR TECNICO ESCRUTINIOS">DIRECTOR TECNICO ESCRUTINIOS</OPTION>
                               <OPTION VALUE="DIRECTOR TECNICO PRECONTEO">DIRECTOR TECNICO PRECONTEO</OPTION>
                               <OPTION VALUE="ELECTORAL">ELECTORAL</OPTION>
                               <OPTION VALUE="MESA DE INVESTIGACION">MESA DE INVESTIGACION</OPTION>
                               <OPTION VALUE="PROCURADOR">PROCURADOR</OPTION>
                               <OPTION VALUE="PATINADOR">PATINADOR</OPTION>
                               <OPTION VALUE="RECOLECTOR DE ACTAS">RECOLECTOR DE ACTAS</OPTION>
                               <OPTION VALUE="RECEPTOR TELEFONICO">RECEPTOR TELEFONICO</OPTION>
                               <OPTION VALUE="SOPORTE TECNICO ESCRUTINIOS">SOPORTE TECNICO ESCRUTINIOS</OPTION>
                               <OPTION VALUE="SOPORTE TECNICO PRECONTEO">SOPORTE TECNICO PRECONTEO</OPTION>
                               <OPTION VALUE="SUPERVISOR DE REGISTRADURIA">SUPERVISOR DE REGISTRADURIA</OPTION>
                               <OPTION VALUE="UNE">UNE</OPTION>
                               <OPTION VALUE="TRANSMISOR TELEFONICO">TRANSMISOR TELEFONICO</OPTION>
                               <OPTION VALUE="VERIFICADOR">VERIFICADOR</OPTION> 
                               
                            </SELECT>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-6 col-md-offset-2">  
                            <input type="file" name ="file" accept=".csv" class="form-control" >
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-6 col-md-offset-2">  
                            <input class="btn btn-primary" type="submit" value ="Generar">
                            <a href="templates/verArchivo.jsp" class="btn btn-primary">Descargar demo.csv</a>
                        </div>
                    </div>
                </form>
             </div>
            </div>
        </div>
    </div>
</div>